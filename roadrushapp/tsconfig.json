{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "emitDeclarationOnly": true,
    "moduleResolution": "Node",
    "allowJs": true,
    "declaration": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ES2017",
      "DOM"
    ],
    "checkJs": true,
    "strict": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
  }
}